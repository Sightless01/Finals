
<body class="bodyDiv">
	<div class="col-md-12">
		<nav class="breadcrumb1">
			<a href="/manage_user" class="body">USER REQUEST</a>
		        <a href="/block" class="body">BLOCK LIST</a>
		    </nav>
	</div>

		
			<div class="container-manage">
				<form method="POST" action="manage">
					<div class="heading"><h2>Proprietor</h2></div>
					<div class="row">
						  <div class="col col-lg-6 col-md-2 col-sm-4">Name</div>
						  <div class="col col-lg-1 col-md-2 col-sm-4">Accept</div>
						  <div class="col col-lg-1 col-md-2 col-sm-4">Reject</div>
					</div>
					<div class="row">
						<div class="col col-lg-6 col-md-2 col-sm-4">
							{{#each companies }}
								<div class="col col-lg-6 col-md-2 col-sm-4">{{ username }}</div>
							{{/each }}
						</div>
						<div class="col col-lg-1 col-md-2 col-sm-4">
							{{#each companies }}
								<div class="col col-lg-6 col-md-2 col-sm-4"><input type="radio" name="btn[p{{comp_id}}]" value="accept"></div>
							{{/each }}
						</div>
						<div class="col col-lg-1 col-md-2 col-sm-4">
							{{#each companies }}
								<div class="col col-lg-6 col-md-2 col-sm-4"><input type="radio" name="btn[p{{comp_id}}]" value="reject"></div>
							{{/each }}
						</div>
						
						
					</div>
					<div class="heading"><h2>Renter</h2></div>
					<div class="row">
						<div class="col col-lg-6 col-md-2 col-sm-4">
							{{#each clients }}
								<div class="col col-lg-6 col-md-2 col-sm-4">{{ username }}</div>
							{{/each }}
						</div>
						<div class="col col-lg-1 col-md-2 col-sm-4">
							{{#each companies }}
								<div class="col col-lg-6 col-md-2 col-sm-4"><input type="radio" name="btn[r{{client_id}}]" value="accept"></div>
							{{/each }}
						</div>
						<div class="col col-lg-1 col-md-2 col-sm-4">
							{{#each companies }}
								<div class="col col-lg-6 col-md-2 col-sm-4"><input type="radio" name="btn[r{{client_id}}]" value="reject"></div>
							{{/each }}
						</div>
					</div>
					<div class="button-manage"><button>submit</button></div>
				</form>
			</div>
</body>
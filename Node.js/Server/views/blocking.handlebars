  {{/each }}
  <h1>Renter</h1>
  {{#each clients }}
      <div class="colors">
        {{ username }}
        <input type="checkbox" name="btn[r{{client_id}}]" value="block">
      </div>
  {{/each }}
  <button>submit</button>
</form>

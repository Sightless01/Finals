<div class="container">
  <form method="POST" action="unblock">
    <div class="heading">
  	  <h1>Proprietor</h1>
  	</div>
    <div class="row">
      <div class="col col-3"></div>
      <div class="col col-5"><b>Name</b></div>
      <div class="col col-4"><b>Unblock</b></div>
    </div>
    {{#each companies }}
        <div class="unblock">
          {{ username }}
          <input type="checkbox" name="btn[p{{comp_id}}]"class="unblockbtn" value="block">
        </div>
        <hr>
    {{/each }}
    <div class="heading">
  	  <h1>Renter</h1>
  	</div>
    {{#each clients }}
        <div class="unblock">
          {{ username }}
          <input type="checkbox"name="btn[r{{client_id}}]" class="unblockbtn" value="unblock">
        </div>
        <hr>
    {{/each }}
    <div class="idname">
      <button>Unblock</button>
    </div>
  </form>
</div>

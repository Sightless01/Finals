<form method="POST" action="block">
  <h1 class="h-block">Proprietor</h1>
  {{#each companies }}
      <div class="unblock">
        {{ username }}
        <input type="checkbox" name="btn[p{{comp_id}}]" value="block">
      </div>
  {{/each }}
  <h1>Renter</h1>
  {{#each clients }}
      <div class="unblock">
        {{ username }}
        <input type="checkbox" name="btn[r{{client_id}}]" value="unblock">
      </div>
  {{/each }}
  <button>submit</button>
</form>
<hr>

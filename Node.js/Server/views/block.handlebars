<div class="container">
  <form method="POST" action="block">
    <h1 class="h-block">Proprietor</h1>
    <div class="row">
      <div class="col-md-3"></div>
      <div class="col-md-5">Name</div>
      <div class="col-md-3">Block</div>
      <div class="col-md-1"></div>
    </div>
    {{#each companies }}
        <div class="block">
          {{ username }}
          <input type="checkbox" name="btn[p{{comp_id}}]" class="unblockbtn" value="block">
        </div>
        <hr>
    {{/each }}
    <h1>Renter</h1>
    {{#each clients }}
        <div class="block">
          {{ username }}
          <input type="checkbox" name="btn[r{{client_id}}]" class="unblockbtn"value="block">
        </div>
        <hr>
    {{/each }}
    <div class="idname">
    <button>Block</button>
    </div>
  </form>
</div>
